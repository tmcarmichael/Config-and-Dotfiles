{ // 9-17-20 --- Github Light theme with customizations
  "editor.fontFamily": "MesloLGS NF",
  "editor.fontSize": 16,
  "editor.renderWhitespace": "all",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": false,
  "editor.cursorStyle": "block",
  // "editor.formatOnSave": true,
  "explorer.confirmDelete": false,
  // "gitlens.keymap": "none",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "python.showStartPage": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": true,
  "workbench.colorTheme": "GitHub Light",
  "workbench.iconTheme": "material-icon-theme",
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontSize": 14,
  "window.zoomLevel": -1,
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#8aa7e0b5",
    "editor.inactiveSelectionBackground": "#aac1ef8f",
    "editor.selectionHighlightBackground": "#f7b42ea3",
    "editor.findMatchBackground": "#00aaff4a",
    "editorLineNumber.foreground": "#6f6f6f",
    "editor.lineHighlightBackground": "#b0b0b066",
    "sideBar.background": "#cbcbcb",
    "list.hoverBackground": "#aaaaaa",
    "list.activeSelectionBackground": "#999999",
    "list.inactiveSelectionBackground": "#999999",
    "list.hoverForeground": "#098cff",
    "list.activeSelectionForeground": "#098cff",
    "list.inactiveSelectionForeground": "#098cff",
    "editorCursor.foreground": "#ff800068",
    "terminalCursor.foreground": "#ff800047",
    "editor.background": "#dddddd",
    "terminal.background": "#d9d9d9",
    "terminal.foreground": "#1c1c1c",
    "terminal.ansiBrightBlack": "#665C54",
    "terminal.ansiBrightBlue": "#6c88ec",
    "terminal.ansiBrightCyan": "#46dba2",
    "terminal.ansiBrightGreen": "#40f100f1",
    "terminal.ansiBrightMagenta": "#8F4673",
    "terminal.ansiBrightRed": "#ff3b21",
    "terminal.ansiBrightWhite": "#7d7d7d",
    "terminal.ansiBrightYellow": "#fdad52",
    "terminal.ansiBlack": "#1f1f1f",
    "terminal.ansiBlue": "#718fefd8",
    "terminal.ansiCyan": "#50d5d7",
    "terminal.ansiGreen": "#5ddc2f",
    "terminal.ansiMagenta": "#8F4673",
    "terminal.ansiRed": "#ff371d",
    "terminal.ansiWhite": "#7d7d7d",
    "terminal.ansiYellow": "#f6ab56",
  },
}
